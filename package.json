{
  "name": "vscode-javascript-typescript-snippets",
  "displayName": "JavaScript and TypeScript Vscode Snippets",
  "description": "Visual Studio Code snippets for JavaScript and TypeScript",
  "version": "0.1.3",
  "publisher": "sankeyangshu",
  "private": true,
  "author": "sankeyangshu",
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/vscode-javascript-snippets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sankeyangshu/vscode-javascript-snippets"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/vscode-javascript-snippets/issues"
  },
  "keywords": [
    "vscode",
    "snippet",
    "javascript",
    "typescript"
  ],
  "categories": [
    "Snippets"
  ],
  "main": "./dist/index.js",
  "icon": "res/logo.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.89.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./dist/snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./dist/snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./dist/snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./dist/snippets/typescript.json"
      },
      {
        "language": "html",
        "path": "./dist/snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./dist/snippets/javascript.json"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/vsce": "^2.26.1",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "esno": "^4.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
